{
  "name": "dxdao-treasury-dashboard",
  "description": "A simple dashboard to track and view the currenct DXdao's treasury across all supported chains.",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "typechain": "typechain --target ethers-v5 --out-dir ./src/typechain ./src/constants/abis/*.json",
    "start": "yarn typechain && react-scripts start",
    "build": "yarn typechain && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@fontsource/montserrat": "^4.4.2",
    "@nivo/core": "^0.70.1",
    "@nivo/pie": "^0.70.1",
    "@typechain/ethers-v5": "^7.0.0",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.0.0",
    "@types/rebass": "^4.0.5",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "@uniswap/token-lists": "^1.0.0-beta.24",
    "decimal.js-light": "^2.5.1",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "ethers": "^5.3.0",
    "husky": "^6.0.0",
    "node-fetch": "^2.6.1",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-loading-skeleton": "^2.2.0",
    "react-scripts": "^4.0.3",
    "rebass": "^4.0.7",
    "styled-components": "^5.3.0",
    "typechain": "^5.0.0",
    "typescript": "^4.3.2"
  }
}
